# sup
alias sup="SUP_LOG_LEVEL=debug ruby -I $HOME/dev/sup/mainline/lib -w $HOME/dev/sup/mainline/bin/sup"
alias sup-add="SUP_LOG_LEVEL=debug ruby -I $HOME/dev/sup/mainline/lib -w $HOME/dev/sup/mainline/bin/sup-add"
alias sup-config="SUP_LOG_LEVEL=debug ruby -I $HOME/dev/sup/mainline/lib -w $HOME/dev/sup/mainline/bin/sup-config"
alias sup-dump="SUP_LOG_LEVEL=debug ruby -I $HOME/dev/sup/mainline/lib -w $HOME/dev/sup/mainline/bin/sup-dump"
alias sup-sync="SUP_LOG_LEVEL=debug ruby -I $HOME/dev/sup/mainline/lib -w $HOME/dev/sup/mainline/bin/sup-sync"

alias heliotrope-add="ruby1.9.1 -I $HOME/dev/sup/heliotrope/lib $HOME/dev/sup/heliotrope/bin/heliotrope-add"
alias heliotrope-console="ruby1.9.1 -I $HOME/dev/sup/heliotrope/lib $HOME/dev/sup/heliotrope/bin/heliotrope-console"
alias heliotrope-import="ruby1.9.1 -I $HOME/dev/sup/heliotrope/lib $HOME/dev/sup/heliotrope/bin/heliotrope-import"
alias heliotrope-server="ruby1.9.1 -I $HOME/dev/sup/heliotrope/lib $HOME/dev/sup/heliotrope/bin/heliotrope-server"
alias turnsole="TURNSOLE_LOG_LEVEL=debug ruby1.9.1 -I $HOME/dev/sup/turnsole/lib -I $HOME/dev/sup/heliotrope/lib $HOME/dev/sup/turnsole/bin/turnsole"

