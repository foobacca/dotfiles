# some more ls aliases
alias ll='ls -l'
alias lh='ls -lh'
alias llh='ls -lh'
alias la='ls -A'
alias l="ls -CF --hide='*.pyc'"
alias lt='ls -laptr' #oldest first sort
alias labc='ls -lap' #alphabetical sort

# cd aliases
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

